  "Swagger": {
    "apiTitle": "Nursing微服务 API",
    "apiName": "ECIS Support API",
    "Name": "ecis-Nursing",
    "ns": "YiJian.Nursing",
    "RootPath": "Nursing"
  },
  "GrpcMasterData": "http://ecis-masterdata:666",
