  </ItemGroup>

  <ItemGroup>
    <Folder Include="MyProjectName\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\shared\YiJian.ECIS.ShareModel\YiJian.ECIS.ShareModel.csproj" />
  </ItemGroup>

</Project>
