<Project Sdk="Microsoft.NET.Sdk">

  <!--<Import Project="..\..\common.props" />-->

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>YiJian.Health.Report</RootNamespace>
  </PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>obj/Debug/net6.0/YiJian.Health.Report.Application.xml</DocumentationFile>
		<OutputPath>bin\Debug</OutputPath>
		<PlatformTarget>x64</PlatformTarget>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<DocumentationFile>obj/Release/net6.0/YiJian.Health.Report.Application.xml</DocumentationFile>
		<PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>
	
	<ItemGroup>
    <PackageReference Include="Com.Ctrip.Framework.Apollo.Configuration" Version="2.9.0" />
    <PackageReference Include="DotNetCore.CAP" Version="6.2.1" />
    <PackageReference Include="DotNetCore.CAP.Dashboard" Version="6.2.1" />
    <PackageReference Include="DotNetCore.CAP.RabbitMQ" Version="6.2.1" />
    <PackageReference Include="DotNetCore.CAP.SqlServer" Version="6.2.1" />
    <PackageReference Include="FastReport.Core" Version="2022.2.2-demo" />
    <PackageReference Include="FastReport.Web" Version="2022.2.2-demo" /> 
    <PackageReference Include="Google.Protobuf" Version="3.19.4" />
    <PackageReference Include="Grpc.Net.Client" Version="2.46.0" />
    <PackageReference Include="Grpc.Net.ClientFactory" Version="2.46.0" />
    <PackageReference Include="Grpc.Tools" Version="2.46.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.VisualBasic" Version="4.0.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="4.0.0" />
    <PackageReference Include="MiniExcel" Version="1.30.0" />
		<PackageReference Include="Volo.Abp.AutoMapper" Version="6.0.3" />
		<PackageReference Include="Volo.Abp.Ddd.Application" Version="6.0.3" />
		<ProjectReference Include="..\..\..\..\shared\YiJian.ECIS.Core\YiJian.ECIS.Core.csproj" />
		<ProjectReference Include="..\..\..\..\shared\YiJian.ECIS.DDP\YiJian.ECIS.DDP.csproj" />
    <ProjectReference Include="..\YiJian.Health.Report.Application.Contracts\YiJian.Health.Report.Application.Contracts.csproj" />
    <ProjectReference Include="..\YiJian.Health.Report.Domain\YiJian.Health.Report.Domain.csproj" />
  </ItemGroup>

	<ItemGroup>
	  <Protobuf Include="..\..\..\..\shared\YiJian.ECIS.Protos\healthReport.proto">
	    <Link>Protos\healthReport.proto</Link>
	  </Protobuf>
    <Protobuf Include="..\..\..\..\shared\YiJian.ECIS.Protos\masterdata.proto" GrpcServices="Client">
      <Link>Protos\masterdata.proto</Link>
    </Protobuf>
  <Protobuf Include="..\..\..\..\shared\YiJian.ECIS.Protos\recipe.proto" GrpcServices="Client">
      <Link>Protos\recipe.proto</Link>
    </Protobuf>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Update="Fody" Version="6.5.3">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

	<ItemGroup>
	  <Reference Include="FastReport">
	    <HintPath>..\..\dll\FastReport.dll</HintPath>
	  </Reference>
	</ItemGroup>

</Project>
